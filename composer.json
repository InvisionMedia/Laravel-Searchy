{
    "name": "invision-media/searchy",
    "description": "Laravel Searchy makes user driven searching easy with fuzzy search, basic string matching, Levenshtein Distance and more. This fork makes it possible to change your database connection.",
    "keywords": ["laravel","search","fuzzy"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Lingham",
            "email": "tjlingham@gmail.com"
        },
        {
            "name": "Jonathan Bird",
            "email": "jonathan@digitalbird.com.au"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^6.0",
        "phpunit/phpunit": "^10.0 || ^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "InvisionMedia\\Searchy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InvisionMedia\\Tests\\Searchy\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "InvisionMedia\\Searchy\\SearchyServiceProvider"
            ],
            "aliases": {
                "Searchy": "InvisionMedia\\Searchy\\Facades\\Searchy"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
